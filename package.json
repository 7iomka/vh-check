{
  "name": "vh-check",
  "version": "1.1.2",
  "description": "iOS vh buggyfill",
  "main": "dist/vh-check.js",
  "scripts": {
    "demo": "npm run build:demo && http-server ./demo",
    "format-code": "prettier --single-quote --trailing-comma es5 src/index.js --write src/index.js",
    "toc": "doctoc README.md --github --notitle",
    "build": "rollup --config",
    "build:demo": "npm run build && npm run build:demo:commonjs && npm run build:demo:esmodule",
    "build:demo:commonjs": "browserify ./demo/commonjs-example.js -o ./demo/commonjs-bundle.js",
    "build:demo:esmodule": "parcel build demo/esmodule-example.js --out-dir demo --out-file esmodule-bundle.js --no-source-maps --no-minify",
    "prepublishOnly": "npm run toc && npm run build",
    "release": "np"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hiswe/vh-check.git"
  },
  "keywords": [
    "ios",
    "mobile",
    "css",
    "vh",
    "vertical-height",
    "buggyfill"
  ],
  "author": "hiswe <hiswehalya@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hiswe/vh-check/issues"
  },
  "homepage": "https://github.com/Hiswe/vh-check#readme",
  "dependencies": {},
  "devDependencies": {
    "browserify": "^16.2.2",
    "doctoc": "^1.3.1",
    "http-server": "^0.11.1",
    "lodash.merge": "^4.6.1",
    "parcel-bundler": "^1.8.1",
    "prettier": "1.13.5",
    "rollup": "^0.60.4",
    "rollup-plugin-uglify": "^4.0.0"
  }
}
