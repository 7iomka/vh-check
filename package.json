{
  "name": "vh-check",
  "version": "1.0.12",
  "description": "iOS vh buggyfill",
  "main": "vh-check.js",
  "scripts": {
    "demo": "npm run build:demo && cp ./vh-check.js ./demo/vh-check.js && http-server ./demo",
    "format-code": "prettier --single-quote --trailing-comma es5 src/index.js --write src/index.js",
    "toc": "doctoc README.md --github --notitle",
    "update-dist-folder": "cp vh-check.js dist/vh-check.js && mv vh-check.min.js dist/vh-check.min.js",
    "build": "rollup --config && npm run update-dist-folder",
    "build:demo": "npm run build && browserify ./demo/index.js -o ./demo/bundle.js",
    "prepublishOnly": "npm run toc && npm run build",
    "release": "np"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hiswe/ios-vh.git"
  },
  "keywords": [
    "ios",
    "css",
    "vh",
    "vertical-height",
    "buggyfill"
  ],
  "author": "hiswe <hiswehalya@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hiswe/ios-vh/issues"
  },
  "homepage": "https://github.com/Hiswe/ios-vh#readme",
  "dependencies": {},
  "devDependencies": {
    "browserify": "^16.1.1",
    "doctoc": "^1.3.1",
    "http-server": "^0.11.1",
    "lodash.merge": "^4.6.1",
    "prettier": "1.11.1",
    "rollup": "^0.57.1",
    "rollup-plugin-cleanup": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0"
  }
}
